<!-- #INCLUDE FILE="library/parametros.asp" --><!-- #INCLUDE FILE="library/conOpen.asp" --><!-- #INCLUDE FILE="library/conOpenSIM.asp" --><!-- #INCLUDE FILE="library/fErroMsg.asp" --><!-- #INCLUDE FILE="library/fAddZero.asp" --><!-- #INCLUDE FILE="library/fFormataValor.asp" --><!-- #INCLUDE FILE="library/fFormataCPF.asp" --><!-- #INCLUDE FILE="library/fData.asp" --><!-- #INCLUDE FILE="library/vHoje.asp" --><%FUNCTION DVAgencia(qNumero) '####Calcula DV para nosso numero, agencia e codigo de cedente####	mNum=9	tempTot=0	FOR i = Len(qNumero) TO 1 STEP -1		tempTot=Int(tempTot)+mNum*Int(MID(qNumero,i,1))		mNum=mNum-1		IF mNum<2 THEN mNum=9	NEXT	tempTot=tempTot-(Fix(tempTot/11)*11)	IF tempTot=10 THEN tempTot="X"	DVAgencia=tempTotEND FUNCTION%><%FUNCTION DVNumBancario(qNumero) '####Calcula DV para nosso numero, agencia e codigo de cedente####	soma=(7*Mid(qNumero,1,1))+(3*Mid(qNumero,2,1))+(1*Mid(qNumero,3,1))+(9*Mid(qNumero,4,1))+(7*Mid(qNumero,5,1))+(3*Mid(qNumero,6,1))+(1*Mid(qNumero,7,1))+(9*Mid(qNumero,8,1))+(7*Mid(qNumero,9,1))+(3*Mid(qNumero,10,1))	soma=CStr(soma)	soma=Mid(soma,Len(soma),1)	IF int(Soma)=0 THEN		soma=0	ELSE		soma=10-Int(soma)	END IF	DVNumBancario=somaEND FUNCTION%><%FUNCTION D1CampoLivre(qNumero) '####Calcula DV para linha digitavel####	mNum=2	tot=0	FOR i = Len(qNumero) TO 1 STEP -1		tempTot=mNum*MID(qNumero,i,1)		IF tempTot>9 THEN			tempTot2=0			tempTot=CStr(tempTot)			FOR i2 = Len(tempTot) TO 1 STEP -1				tempTot2=tempTot2+MID(tempTot,i2,1)			NEXT			tempTot=tempTot2		END IF		tot=tot+tempTot		IF mNum=2 THEN mNum=1 ELSE mNum=2 END IF	NEXT	dezena=(Fix(tot/10)+1)*10	tot=dezena-tot	IF tot=10 THEN tot=0	D1CampoLivre=totEND FUNCTION%><%FUNCTION DCampoLivre(qNumero) '####Calcula DV para linha digitavel####	mNum=2	tot=0	FOR i = Len(qNumero) TO 1 STEP -1		tempTot=mNum*MID(qNumero,i,1)		IF tempTot>9 THEN			tempTot=tempTot-9		END IF		tot=tot+tempTot		IF mNum=2 THEN mNum=1 ELSE mNum=2 END IF	NEXT	fracao=tot-(Fix(tot/10)*10)	IF fracao = 0 THEN		tot=0	ELSE		tot=10-fracao	END IF	DCampoLivre=totEND FUNCTION%><%FUNCTION D2CampoLivre(qNumero) '####Calcula DV do codigo de barras####	mNum=2	tempTot=0	FOR i = Len(qNumero) TO 1 STEP -1		tempTot=Int(tempTot)+mNum*Int(MID(qNumero,i,1))		mNum=mNum+1		IF mNum>7 THEN mNum=2	NEXT	tempTot=tempTot-(Fix(tempTot/11)*11)	IF tempTot>1 THEN		tempTot=11-tempTot	END IF	D2CampoLivre=tempTotEND FUNCTION%><%FUNCTION DVcodBarras(qNumero) '####Calcula DV do codigo de barras####	mNum=2	tempTot=0	FOR i = Len(qNumero) TO 1 STEP -1		tempTot=Int(tempTot)+mNum*Int(MID(qNumero,i,1))		mNum=mNum+1		IF mNum>9 THEN mNum=2	NEXT	tempTot=tempTot-(Fix(tempTot/11)*11)	tempTot=11-tempTot	IF tempTot=10 OR tempTot=11 THEN tempTot=1	DVcodBarras=tempTotEND FUNCTION%><%FUNCTION formataCodBarras(qNumero) '####formata codigo de barras para impressao na boleta####	barcodeArray=Array("11331","31113","13113","33111","11313","31311","13311","11133","31131","13131")	codBarrasEnc=""	cor="p"	FOR i = 1 TO Len(qNumero) STEP 2		curNum1=INT(MID(qNumero,i,1))		curNum2=INT(MID(qNumero,i+1,1))		widthStr1=barcodeArray(curNum1)		widthStr2=barcodeArray(curNum2)		FOR size = 1 TO 5			codBarrasEnc=codBarrasEnc&"<img src=""img/"&cor&".gif"" width="&MID(widthStr1,size,1)&" height=50 border=0>"			cor="b"			codBarrasEnc=codBarrasEnc&"<img src=""img/"&cor&".gif"" width="&MID(widthStr2,size,1)&" height=50 border=0>"			cor="p"		NEXT	NEXT	codBarrasEnc="<img src=""img/p.gif"" width=1 height=50 border=0><img src=""img/b.gif"" width=1 height=50 border=0><img src=""img/p.gif"" width=1 height=50 border=0><img src=""img/b.gif"" width=1 height=50 border=0>"&codBarrasEnc&"<img src=""img/p.gif"" width=3 height=50 border=0><img src=""img/b.gif"" width=1 height=50 border=0><img src=""img/p.gif"" width=1 height=50 border=0>"	formataCodBarras=codBarrasEncEND FUNCTION%><%cod = Request("cod")coda = session("id")parc = Request("parc")if isNumeric(cod)=false or cod="" or isNumeric(coda)=false or coda="" or isNumeric(parc)=false or parc="" then erroMsg("Erro no processamento. Favor fazer novo login, se o erro persistir entrar em contato com o IBAM.")cod = int(cod)coda = int(coda)parc = int(parc)sql = "SELECT T.DT_INICIO_TURMA,T.PARCELAS AS CURSO_PARCELAS,CA.PARCELAS AS ALUNO_PARCELAS,CUSTO_ENTI,ASSOCIADO,T.VALOR,C.TITULO,CA.COD_ENTI,T.ID_TURMA FROM TURMAS T JOIN TURMA_ALUNO CA ON T.COD_CURSO=CA.COD_CURSO JOIN CURSO C ON C.COD_CURSO=T.COD_CURSO WHERE T.ID_TURMA="&cod&" AND CA.COD_ALUNO="&coda'response.write sql'response.endRS.Open sqlsql = "UPDATE TURMA_ALUNO SET STATUS='INSCRITO' WHERE ID_TURMA="&cod&" AND COD_ALUNO="&codaCon.Execute sqlif RS.EOF then	erroMsg("Erro no processamento. Se o erro persistir entrar em contato com o IBAM.")else	CURSO_PARCELAS = RS("CURSO_PARCELAS")	ALUNO_PARCELAS = RS("ALUNO_PARCELAS")	DT_INICIO_TURMA = RS("DT_INICIO_TURMA")	CUSTO_ENTI = RS("CUSTO_ENTI")	ASSOCIADO = RS("ASSOCIADO")	VALOR = RS("VALOR")	ID_TURMA = RS("ID_TURMA")	TITULO = UCASE(RS("TITULO"))	COD_ENTI = RS("COD_ENTI")end ifRS.Closeif CUSTO_ENTI then	RSSIM.Open "SELECT NOME,EMPPRIV_CNPJ,NOME_MUNI,SIGLA_UF FROM ENTIDADES E JOIN MUNICIPIOS M ON E.COD_UF_IBGE=M.COD_UF_IBGE AND E.COD_MUNI_IBGE=M.COD_MUNI_IBGE JOIN UF ON M.COD_UF_IBGE=UF.COD_UF_IBGE WHERE COD_ENTI="&COD_ENTI	if not RSSIM.EOF then		NOME = UCASE(RSSIM("NOME") & " (" &  RSSIM("NOME_MUNI") & "/" & RSSIM("SIGLA_UF") & ")")		CPF = RSSIM("EMPPRIV_CNPJ")	end if	RSSIM.Closeelse	RS.Open "SELECT NOME,CPF FROM ALUNO WHERE COD_ALUNO="&coda	NOME = UCASE(RS("NOME"))	CPF = formataCPF(RS("CPF"))	RS.Closeend ifif parc<1 or parc>ALUNO_PARCELAS then erroMsg("Número da parcela inválida. Se o erro persistir entrar em contato com o IBAM.")RS.Open "SELECT COUNT(*) FROM PAGAMENTO WHERE COD_CURSO="&cod&" AND COD_ALUNO="&coda&" AND NUM_PARCELA="&parcexiste=RS(0)RS.Closeif existe=0 then	con.execute "INSERT INTO PAGAMENTO (COD_CURSO,COD_ALUNO,NUM_PARCELA,ID_TURMA) VALUES ("&cod&","&coda&","&parc&","&ID_TURMA&")"end ifRS.Open "SELECT COD_PAGAMENTO FROM PAGAMENTO WHERE COD_CURSO="&cod&" AND COD_ALUNO="&coda&" AND NUM_PARCELA="&parcCOD_PAGAMENTO=RS(0)RS.Close'valor do boleto e data de validadeif ALUNO_PARCELAS=1 then	if CUSTO_ENTI and ASSOCIADO then		VALOR = Round(VALOR * .8)		dataVal=DT_INICIO_TURMA		txt = "O VALOR DO BOLETO INCLUI OS 20% DE DESCONTO AOS ASSOCIADOS DO IBAM COM PAGAMENTO À VISTA ATÉ A DATA DE VENCIMENTO DESTE BOLETO"	else		if DT_INICIO_TURMA>=hoje then			VALOR = Round(VALOR * .9)			dataVal=DT_INICIO_TURMA			txt = "O VALOR DO BOLETO INCLUI OS 10% DE DESCONTO PARA PAGAMENTO À VISTA ATÉ A DATA DE VENCIMENTO DESTE BOLETO"		else			VALOR = VALOR			dataVal=addDate("m",1,hoje)			txt = "VALOR PARA PAGAMENTO À VISTA ATÉ A DATA DE VENCIMENTO DESTE BOLETO"		end if	end ifelse	if CUSTO_ENTI and ASSOCIADO then VALOR = Round(VALOR * .8)	valor_parcela = Round(VALOR/ALUNO_PARCELAS)	if parc=ALUNO_PARCELAS then		VALOR = VALOR - (valor_parcela*(ALUNO_PARCELAS-1))	else		VALOR = valor_parcela	end if	txt = "VALOR REFERENTE À PARCELA "&parc&" DE UM TOTAL DE "&ALUNO_PARCELAS&" PARCELAS. PAGAR ATÉ O VENCIMENTO"	dataVal=addDate("m",1,hoje)end ifdataValPrint=dataValdataValPrint=formataData(dataValPrint)dataDocumento=hojedataDocumento=formataData(dataDocumento)dataProcessamento=dataDocumentofatorVencimento=DateDiff("d","7-"&MonthName(10,TRUE)&"-1997",MID(dataVal,9,2)&"-"&MonthName(MID(dataVal,6,2))&"-"&MID(dataVal,1,4))IF fatorVencimento>9999 THEN '####erro de fator de vencimento####	erro="Fator de vencimento inválido! Por ordem do Banespa, o sistema só imprime boletas com data de validade menor do que 22/02/2025."END IFfatorVencimento=addZero(fatorVencimento,4)nDocumento=COD_PAGAMENTOnDocumento=addZero(nDocumento,9)valorPrint=VALORvalorPrint=addZero(valorPrint,10)codBanco="237"codMoeda="9"codBancoPrint=codBanco&"-2"agencia="3369"agenciaDV="3"CEDENTE="IBAM-Instituto Brasileiro de Administração Municipal"CEDENTE_CNPJ="33.645.482/0001-96"codCedente="0000013"codCedenteDV="2"codCedentePrint=agencia&"-"&agenciaDV&"/"&codCedente&"-"&codCedenteDVcarteira="06"nossoNumero=addZero(COD_PAGAMENTO,11)nossoNumeroDV=carteira&nossoNumeronossoNumeroDV=D2CampoLivre(nossoNumeroDV)nossoNumeroPrint=carteira&"/"&nossoNumero&"-"&nossoNumeroDVcampoLivre=agencia&carteira&nossoNumero&codCedente&"0"codBarras=codBanco&codMoeda&fatorVencimento&valorPrint&campoLivrecodBarrasDV=codBarrascodBarrasDV=DVcodBarras(codBarrasDV)codBarras=MID(codBarras,1,4)&codBarrasDV&MID(codBarras,5)IF Len(codBarras)<>44 THEN '####erro de codigo de barras####	erro="Erro no código de barras, Favor contactar o IBAM."END IFcodBarras=formataCodBarras(codBarras)linhaDigC1=codBanco&codMoeda&MID(campoLivre,1,5)linhaDigC2=MID(campoLivre,6,10)linhaDigC3=MID(campoLivre,16,10)linhaDigC5=CStr(fatorVencimento)&valorPrintlinhaDigitavel=MID(linhaDigC1,1,5)&"."&MID(linhaDigC1,6)&DCampoLivre(linhaDigC1)&" "&MID(linhaDigC2,1,5)&"."&MID(linhaDigC2,6)&DCampoLivre(linhaDigC2)&" "&MID(linhaDigC3,1,5)&"."&MID(linhaDigC3,6)&DCampoLivre(linhaDigC3)&" "&codBarrasDV&" "&linhaDigC5%><!DOCTYPE HTML PUBLIC '-//W3C//DTD HTML 4.0 Transitional//EN'><html><HEAD><meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1"><TITLE>ENSUR Inscrições</TITLE><LINK REL="STYLESHEET" TYPE="text/CSS" HREF="style2.css"><meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1"><meta http-equiv=Content-Type content=text/html; charset=windows-1252><style type=text/css><!--.cp { font: bold 10px Arial; color: black }--><!--.ti { font: 9px Arial, Helvetica, sans-serif }--><!--.ld { font: bold 14px Arial; color: #000000 }--><!--.ct { FONT: 9px "Arial Narrow"; COLOR: #000033 }--><!--.cn { FONT: 9px Arial; COLOR: black }--><!--.bc { font: bold 22px Arial; color: #000000 }--></style></head><body text=#000000 bgColor=#ffffff topMargin=0 rightMargin=0><table cel